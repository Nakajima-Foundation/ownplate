{
  "name": "OwnPlate",
  "version": "3.0.7",
  "private": true,
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build && cp dist/index.html functions/templates/index.html",
    "batch": "babel-node --presets @babel/env",
    "lint": "yarn run dumpi18n && eslint src",
    "dumpi18n": "npx tsx batch/i18n-dump.ts",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "format": "prettier --write '{src/**/*,*}.{js,ts,vue,json,css}'",
    "makenews": "npx tsx ./batch/news.js",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@line/liff": "^2.27.2",
    "@sentry/tracing": "^7.120.4",
    "@sentry/vue": "^10.25.0",
    "@unhead/vue": "^2.0.17",
    "chart.js": "^4.5.1",
    "cookie": "^1.0.2",
    "crypto-js": "^4.1.1",
    "firebase": "12.5.0",
    "google-libphonenumber": "^3.2.43",
    "markdown-it": "^14.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.6.0",
    "pdfmake": "0.3.0-beta.17",
    "pinia": "^3.0.4",
    "qrcode": "^1.5.4",
    "query-string": "^9.3.1",
    "unplugin-vue-markdown": "^29.2.0",
    "validator": "^13.15.23",
    "vite": "^7.2.2",
    "vue": "^3.5.24",
    "vue-chart-3": "^3.1.8",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.6.3",
    "vue-social-sharing": "^4.0.0-alpha4"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^4.1.0",
    "@tailwindcss/vite": "^4.1.17",
    "@types/cookie": "^1.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/google-libphonenumber": "^7.4.27",
    "@types/google.maps": "^3.58.1",
    "@types/gtag.js": "^0.0.20",
    "@types/pdfmake": "^0.2.12",
    "@types/query-string": "^6.3.0",
    "@types/validator": "^13.15.8",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.5.1",
    "firebase-admin": "^13.6.0",
    "jsonc-eslint-parser": "^2.4.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "raw-loader": "^4.0.2",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite-plugin-checker": "^0.11.0",
    "vue-eslint-parser": "^10.2.0",
    "yaml-eslint-parser": "^1.3.0"
  }
}
