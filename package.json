{
  "name": "OwnPlate",
  "version": "1.0.0",
  "description": "OwnPlate - Delicious food",
  "author": "FujiyamaYuta",
  "private": true,
  "scripts": {
    "dev": "nuxt --port 3000",
    "devsync": "HOST=192.168.100.20 PORT=3333 nuxt",
    "build:functions": "tsc --project functions",
    "build": "nuxt build && cp dist/index.html functions/templates/index.html",
    "start": "axios-mock-server -b && nuxt start",
    "generate": "axios-mock-server -b && nuxt generate",
    "emulate": "npm run build:functions && firebase emulators:start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "deploy_staging": "nuxt build &&  cp dist/index.html functions/templates/index.html && firebase deploy --only hosting --project=staging && firebase deploy --only functions --project=staging",
    "deploy": "nuxt build && cp dist/index.html functions/templates/index.html &&  firebase deploy --only hosting --project=staging && firebase deploy --only functions --project=staging",
    "deploy:dev": "nuxt build && cp dist/index.html functions/templates/index.html &&  firebase deploy --only hosting --project=default && firebase deploy --only functions --project=default",
    "deploy_fs_rule": "firebase deploy --only firestore:rules --project=staging",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/firebase.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/firebase.js"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@johmun/vue-tags-input": "^2.1.0",
    "@nuxtjs/axios": "^5.9.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "axios-mock-server": "^0.16.3",
    "babel-polyfill": "^6.26.0",
    "bulma-social": "^1.0.1",
    "firebase": "^7.12.0",
    "google-libphonenumber": "^3.2.8",
    "nuxt": "^2.0.0",
    "nuxt-buefy": "^0.3.2",
    "nuxt-gmaps": "^1.1.9",
    "nuxt-i18n": "^6.6.1",
    "validator": "^13.0.0",
    "vue-carousel": "^0.18.0",
    "vue-croppa": "^1.3.8",
    "vuex-persistedstate": "^2.7.0"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.13.0",
    "nuxt-sass-resources-loader": "^2.0.5",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.0",
    "typescript": "^3.8.3"
  }
}
