{
  "name": "OwnPlate",
  "version": "2.0.24",
  "private": true,
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "scripts": {
    "build": "vue-cli-service build && cp dist/index.html functions/templates/index.html",
    "batch": "babel-node --presets @babel/env",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "format": "prettier --write 'src/**/*.{js,ts,vue}'",
    "makenews": "babel-node --presets @babel/env  ./batch/news.js",
    "start": "vue-cli-service serve",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@fawmi/vue-google-maps": "^0.9.79",
    "@line/liff": "^2.18.2",
    "@oruga-ui/oruga-next": "^0.5.10",
    "@oruga-ui/theme-bulma": "^0.2.8",
    "@sentry/tracing": "^6.19.4",
    "@sentry/vue": "^6.19.4",
    "@tailwindcss/line-clamp": "^0.4.2",
    "bulma": "^0.9.4",
    "caniuse-lite": "^1.0.30001393",
    "chart.js": "^3.9.1",
    "cookie": "^0.4.2",
    "firebase": "^9.15.0",
    "google-libphonenumber": "^3.2.30",
    "markdown-it": "^12.3.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "node-fetch": "^2.6.7",
    "pdfmake": "0.3.0-beta.2",
    "qrcode": "^1.5.1",
    "validator": "^13.7.0",
    "vue": "^3.2.45",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.1.6",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.8",
    "@google-cloud/firestore": "^5.0.2",
    "@types/cookie": "^0.4.1",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/gtag.js": "^0.0.10",
    "@types/lodash": "^4.14.181",
    "@types/pdfmake": "^0.1.21",
    "@types/validator": "^13.7.6",
    "@vue/cli": "^5.0.8",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "autoprefixer": "^10.4.4",
    "csv-parse": "^5.2.0",
    "csv-stringify": "^6.1.3",
    "firebase-admin": "^11.4.1",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.8.3"
  }
}
