{
  "name": "OwnPlate",
  "version": "3.0.4",
  "private": true,
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build && cp dist/index.html functions/templates/index.html",
    "batch": "babel-node --presets @babel/env",
    "lint": "eslint src",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "format": "prettier --write '{src/**/*,*}.{js,ts,vue,json}'",
    "makenews": "npx tsx ./batch/news.js",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@fawmi/vue-google-maps": "^0.9.79",
    "@line/liff": "^2.25.1",
    "@oruga-ui/oruga-next": "^0.9.3",
    "@oruga-ui/theme-bulma": "^0.5.3",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vue": "^8.50.0",
    "@unhead/vue": "^1.11.18",
    "caniuse-lite": "^1.0.30001695",
    "chart.js": "^3.9.1",
    "cookie": "^0.7.2",
    "crypto-js": "^4.1.1",
    "firebase": "11.2.0",
    "google-libphonenumber": "^3.2.40",
    "markdown-it": "^14.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.46",
    "node-fetch": "^2.6.7",
    "pdfmake": "0.3.0-beta.2",
    "qrcode": "^1.5.4",
    "query-string": "^9.1.1",
    "unplugin-vue-markdown": "^28.1.0",
    "validator": "^13.7.0",
    "vite": "^6.0.7",
    "vue": "^3.5.13",
    "vue-chart-3": "^3.1.8",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.5.0",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vue3-google-map": "^0.21.1",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/crypto-js": "^4.1.1",
    "@types/google-libphonenumber": "^7.4.27",
    "@types/google.maps": "^3.58.1",
    "@types/gtag.js": "^0.0.20",
    "@types/pdfmake": "^0.2.11",
    "@types/query-string": "^6.3.0",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-typescript": "^14.3.0",
    "autoprefixer": "^10.4.20",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-vue": "^9.32.0",
    "firebase-admin": "^13.0.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "raw-loader": "^4.0.2",
    "sass": "^1.83.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite-plugin-checker": "^0.8.0",
    "vue-eslint-parser": "^9.4.3"
  }
}
