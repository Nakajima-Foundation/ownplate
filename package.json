{
  "name": "OwnPlate",
  "version": "2.0.0",
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "emulate": "npm --prefix functions run build && firebase emulators:start",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "makenews": "babel-node --presets @babel/env  ./batch/news.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js",
    "userlog": "babel-node --presets @babel/env  ./batch/generate-user-log.js",
    "ppp": "babel-node --presets @babel/env  ./batch/setParent.js",
    "batch": "babel-node --presets @babel/env",
    "deploy_staging": "nuxt build &&  cp dist/index.html functions/templates/index.html && firebase deploy --only hosting --project=staging && firebase deploy --only functions --project=staging",
    "deploy": "nuxt build && cp dist/index.html functions/templates/index.html &&  firebase deploy --only hosting --project=staging && firebase deploy --only functions --project=staging",
    "deploy:dev": "nuxt build && cp dist/index.html functions/templates/index.html &&  firebase deploy --only hosting --project=default && firebase deploy --only functions --project=default",
    "deploy_fs_rule": "firebase deploy --only firestore:rules --project=staging",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_eu": "firebase use eu && cp src/config/default/ownplate-eu.js src/config/project.js",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@chenfengyuan/vue-qrcode": "^1.0.2",
    "@johmun/vue-tags-input": "^2.1.0",
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "babel-polyfill": "^6.26.0",
    "bulma-social": "^1.0.1",
    "cookie": "^0.4.1",
    "firebase": "^8.2.4",
    "google-libphonenumber": "^3.2.10",
    "lodash": "^4.17.20",
    "markdown-it": "^11.0.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "node-fetch": "^2.6.1",
    "pdfmake": "^0.1.69",
    "validator": "^13.0.0",
    "vue": "^2.6.12",
    "vue-clipboard2": "^0.3.1",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.17.4",
    "vue-router": "^3.5.2",
    "vue-social-sharing": "^2.4.7",
    "vuex": "^3.6.2",
    "y18n": "^5.0.8"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-optional-catch-binding": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.19.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "firebase-admin": "^9.4.2",
    "ini": ">=1.3.6",
    "node-sass": "^6.0.1",
    "postcss": "^7.0.35",
    "prettier": "^1.19.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^10.2.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3",
    "vue-template-compiler": "^2.5.21"
  }
}
