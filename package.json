{
  "name": "OwnPlate",
  "version": "3.0.7",
  "private": true,
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build && cp dist/index.html functions/templates/index.html",
    "batch": "babel-node --presets @babel/env",
    "lint": "eslint src",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "format": "prettier --write '{src/**/*,*}.{js,ts,vue,json}'",
    "makenews": "npx tsx ./batch/news.js",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@line/liff": "^2.27.0",
    "@oruga-ui/oruga-next": "^0.11.1",
    "@oruga-ui/theme-bulma": "^0.7.0",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vue": "^9.35.0",
    "@unhead/vue": "^2.0.12",
    "chart.js": "^3.9.1",
    "cookie": "^1.0.2",
    "crypto-js": "^4.1.1",
    "firebase": "11.10.0",
    "google-libphonenumber": "^3.2.42",
    "markdown-it": "^14.1.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.6.0",
    "pdfmake": "0.3.0-beta.2",
    "qrcode": "^1.5.4",
    "query-string": "^9.2.2",
    "unplugin-vue-markdown": "^29.1.0",
    "validator": "^13.15.15",
    "vite": "^7.0.2",
    "vue": "^3.5.17",
    "vue-chart-3": "^3.1.8",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^11.1.9",
    "vue-router": "^4.5.1",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@types/cookie": "^1.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/google-libphonenumber": "^7.4.27",
    "@types/google.maps": "^3.58.1",
    "@types/gtag.js": "^0.0.20",
    "@types/pdfmake": "^0.2.11",
    "@types/query-string": "^6.3.0",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.3.0",
    "firebase-admin": "^13.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "raw-loader": "^4.0.2",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.36.0",
    "vite-plugin-checker": "^0.10.0",
    "vue-eslint-parser": "^10.2.0"
  }
}
