{
  "name": "OwnPlate",
  "version": "3.0.4",
  "private": true,
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "scripts": {
    "start": "vite",
    "build": "vite build && cp dist/index.html functions/templates/index.html",
    "batch": "babel-node --presets @babel/env",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "format": "prettier --write '{src/**/*,*}.{js,ts,vue,json}'",
    "makenews": "babel-node --presets @babel/env  ./batch/news.js",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@fawmi/vue-google-maps": "^0.9.79",
    "@line/liff": "^2.18.2",
    "@oruga-ui/oruga-next": "^0.6.0",
    "@oruga-ui/theme-bulma": "^0.2.11",
    "@sentry/tracing": "^6.19.4",
    "@sentry/vue": "^6.19.4",
    "@tailwindcss/line-clamp": "^0.4.4",
    "bulma": "^0.9.4",
    "caniuse-lite": "^1.0.30001520",
    "chart.js": "^3.9.1",
    "cookie": "^0.4.2",
    "crypto-js": "^4.1.1",
    "firebase": "9.15.0",
    "google-libphonenumber": "^3.2.33",
    "markdown-it": "^12.3.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "node-fetch": "^2.6.7",
    "pdfmake": "0.3.0-beta.2",
    "qrcode": "^1.5.1",
    "query-string": "^8.1.0",
    "validator": "^13.7.0",
    "vue": "^3.3.4",
    "vue-chart-3": "^3.1.8",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.2.5",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@types/cookie": "^0.4.1",
    "@types/crypto-js": "^4.1.1",
    "@types/google-libphonenumber": "^7.4.27",
    "@types/pdfmake": "^0.1.21",
    "@types/query-string": "^6.3.0",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "csv-parse": "^5.2.0",
    "csv-stringify": "^6.1.3",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "firebase-admin": "^11.10.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "raw-loader": "^4.0.2",
    "sass": "^1.68.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2"
  }
}
