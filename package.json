{
  "name": "OwnPlate",
  "version": "3.0.4",
  "private": true,
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "scripts": {
    "start": "vite",
    "build": "vite build && cp dist/index.html functions/templates/index.html",
    "batch": "babel-node --presets @babel/env",
    "lint": "eslint src",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "format": "prettier --write '{src/**/*,*}.{js,ts,vue,json}'",
    "makenews": "npx tsx ./batch/news.js",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^2.0.0",
    "@fawmi/vue-google-maps": "^0.9.79",
    "@line/liff": "^2.18.2",
    "@oruga-ui/oruga-next": "0.8.2",
    "@oruga-ui/theme-bulma": "^0.2.11",
    "@sentry/tracing": "^6.19.4",
    "@sentry/vue": "^6.19.4",
    "caniuse-lite": "^1.0.30001643",
    "chart.js": "^3.9.1",
    "cookie": "^0.4.2",
    "crypto-js": "^4.1.1",
    "firebase": "10.12.4",
    "google-libphonenumber": "^3.2.33",
    "markdown-it": "^12.3.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "node-fetch": "^2.6.7",
    "pdfmake": "0.3.0-beta.2",
    "qrcode": "^1.5.1",
    "query-string": "^8.1.0",
    "unplugin-vue-markdown": "^0.25.2",
    "validator": "^13.7.0",
    "vite": "^5.3.4",
    "vue": "3.3.13",
    "vue-chart-3": "^3.1.8",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^9.2.2",
    "vue-meta": "^3.0.0-alpha.10",
    "vue-router": "^4.2.5",
    "vue-social-sharing": "^4.0.0-alpha4",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "@types/crypto-js": "^4.1.1",
    "@types/google-libphonenumber": "^7.4.27",
    "@types/gtag.js": "^0.0.20",
    "@types/pdfmake": "^0.2.9",
    "@types/query-string": "^6.3.0",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue/eslint-config-typescript": "^13.0.0",
    "autoprefixer": "^10.4.16",
    "csv-parse": "^5.2.0",
    "csv-stringify": "^6.1.3",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.27.0",
    "firebase-admin": "^12.3.1",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "raw-loader": "^4.0.2",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.1.0",
    "vite-plugin-checker": "^0.7.2",
    "vue-eslint-parser": "^9.4.3"
  }
}
