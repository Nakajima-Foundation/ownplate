{
  "name": "OwnPlate",
  "version": "2.0.0",
  "description": "OwnPlate - Delicious food",
  "author": "OwnPlate developer team",
  "private": true,
  "scripts": {
    "build": "vue-cli-service build && cp dist/index.html functions/templates/index.html",
    "start": "vue-cli-service serve",
    "format": "prettier --write 'src/**/*.{js,ts,vue}'",
    "customclaims": "babel-node --presets @babel/env  ./batch/customclaims.js",
    "makenews": "babel-node --presets @babel/env  ./batch/news.js",
    "user": "babel-node --presets @babel/env  ./batch/user.js",
    "batch": "babel-node --presets @babel/env",
    "use_staging": "firebase use staging && cp src/config/default/ownplate-dev.js src/config/project.js",
    "use_production": "firebase use production && cp src/config/default/ownplate.js src/config/project.js",
    "use_jp": "firebase use jp && cp src/config/default/ownplate-jp.js src/config/project.js"
  },
  "dependencies": {
    "@chenfengyuan/vue-qrcode": "^1.0.2",
    "@googlemaps/js-api-loader": "^1.13.10",
    "@johmun/vue-tags-input": "^2.1.0",
    "@line/liff": "^2.18.2",
    "@sentry/browser": "^5.15.5",
    "@sentry/integrations": "^5.15.5",
    "buefy": "^0.9.19",
    "bulma-social": "^1.0.1",
    "cookie": "^0.4.1",
    "firebase": "^9.6.10",
    "google-libphonenumber": "^3.2.10",
    "lodash": "^4.17.20",
    "markdown-it": "^12.3.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "node-fetch": "^2.6.1",
    "pdfmake": "0.3.0-beta.1",
    "validator": "^13.7.0",
    "vue": "^2.6.14",
    "vue-clipboard2": "^0.3.1",
    "vue-croppa": "^1.3.8",
    "vue-i18n": "^8.17.4",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.5.1",
    "vue-social-sharing": "^3.0.8",
    "vuex": "^3.6.2",
    "y18n": "^5.0.8"
  },
  "devDependencies": {
    "@babel/node": "^7.16.8",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.8",
    "@vue/cli": "^5.0.4",
    "@vue/cli-service": "^5.0.4",
    "autoprefixer": "^10.4.4",
    "firebase-admin": "^10.0.2",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "raw-loader": "^4.0.2",
    "sass-loader": "^12.6.0",
    "tailwindcss": "^3.0.23"
  }
}
